fun main() 
{
    val colors = listOf("green", "orange", "blue")
    
    println("Reversed list:${colors.reversed()}")
    println("List: $colors")
    println("")
    println("Sorted list:${colors.sorted()}")
    println("List: $colors")
    println("")
    val oddNumbers = listOf(5,3,7,1)
    println("List:$oddNumbers")
    println("Sorted list: ${oddNumbers.sorted()}")
}
